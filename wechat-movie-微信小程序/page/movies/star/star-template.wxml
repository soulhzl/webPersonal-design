<!--page/movies/star/star-template.wxml-->
<template name='star'>
  <view class='star-container'>
    <block wx:key="third" wx:for="{{stars}}" wx:for-item='i'>
      <image wx:if='{{i}}' src='/img/icon/star.png'></image>
      <image wx:else src='/img/icon/none-star.png'></image>
    </block>
    <text>{{score}}</text>
  </view>
</template>